<% if can? :read, PressKitLink.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :press_kit_links]),
    label: t('press_kit_links.navigation.label'),
  } do |p| %>
    <% p.content_for :icon do %>
      <i class="<%= t('press_kit_links.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
