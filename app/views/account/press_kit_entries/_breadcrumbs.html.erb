<% press_kit_entry ||= @press_kit_entry %>
<% setting_press_kit ||= @setting_press_kit || press_kit_entry&.setting_press_kit %>
<%= render 'account/setting_press_kits/breadcrumbs', setting_press_kit: setting_press_kit %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, setting_press_kit, :press_kit_entries] %>
<% if press_kit_entry&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: press_kit_entry.label_string, url: [:account, press_kit_entry] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'press_kit_entries' %>
